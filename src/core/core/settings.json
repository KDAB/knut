{
    "lsp": {
        "enabled": true,
        "servers": [
            {
                "type": "cpp_type",
                "program": "clangd",
                "arguments": []
            }
        ]
    },
    "rc": {
        "dialog_flags": [
            "UpdateGeometry",
            "UseIdForPixmap"
        ],
        "dialog_scalex": 1.5,
        "dialog_scaley": 1.65,
        "asset_flags": [
            "RemoveUnknown",
            "SplitToolBar",
            "ConvertToPng"
        ],
        "asset_transparent_colors": [
            "Gray",
            "Magenta",
            "BottomLeftPixel"
        ],
        "language_map": {
            "LANG_NEUTRAL": "[default]"
        }
    },
    "cpp": {
        "excluded_macros": [
            "AFX_EXT_CLASS",
            "[A-Z_]*EXPORT[A-Z_]*",
            "Q_OBJECT"
        ]
    },
    "mime_types": {
        "c": "cpp_type",
        "cpp": "cpp_type",
        "cxx": "cpp_type",
        "h": "cpp_type",
        "hpp": "cpp_type",
        "txt": "text_type",
        "json": "json_type",
        "rc": "rc_type",
        "rc2": "rc_type",
        "ui": "qtui_type",
        "png": "image_type",
        "bmp": "image_type",
        "jpg": "image_type",
        "ico": "image_type",
        "slint": "slint_type",
        "qml": "qml_type",
        "cs": "csharp_type",
        "ts": "qtts_type"
    },
    "text_editor": {
        "tab": {
            "insertSpaces": true,
            "tabSize": 4
        }
    },
    "toggle_section": {
        "tag": "KDAB_TEMPORARILY_REMOVED",
        "debug": "qDebug(\"%1 is commented out\")",
        "return_values": {
            "BOOL": "false",
            "int": "-1"
        }
    },
    "json_paths": [
        ":/scripts/json/"
    ],
    "logs": {
        "saveToFile": false
    }
}
